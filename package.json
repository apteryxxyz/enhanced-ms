{
    "name": "enhanced-ms",
    "version": "2.0.0",
    "license": "MIT",
    "description": "Convert human readable time-frame strings to milliseconds and vice versa, with support for i10n and operators!",
    "homepage": "https://github.com/apteryxxyz/enhanced-ms#readme",
    "author": {
        "name": "ApteryxXYZ",
        "url": "https://apteryx.xyz"
    },
    "main": "./dist/index.js",
    "exports": {
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
    },
    "files": [
        "dist"
    ],
    "packageManager": "yarn@3.2.3",
    "scripts": {
        "lint": "eslint src --fix",
        "format": "prettier src --write",
        "test": "jest",
        "build:compile": "tsc && webpack && rm -rf temp",
        "build:module": "gen-esm-wrapper . ./dist/index.mjs",
        "build": "yarn build:compile && yarn build:module"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/apteryxxyz/enhanced-ms"
    },
    "bugs": {
        "url": "https://github.com/apteryxxyz/enhanced-ms/issues"
    },
    "keywords": [
        "convert",
        "duration",
        "human",
        "humanise",
        "humanize",
        "microseconds",
        "milliseconds",
        "ms",
        "number",
        "parse",
        "period",
        "pretty",
        "print",
        "range",
        "readable",
        "seconds",
        "string",
        "time"
    ],
    "devDependencies": {
        "@babel/core": "^7.19.1",
        "@babel/preset-env": "^7.19.1",
        "@types/babel__core": "^7",
        "@types/babel__preset-env": "^7",
        "@types/node": "^18.7.21",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "babel-loader": "^8.2.5",
        "bundle-declarations-webpack-plugin": "^3.1.1",
        "eslint": "^8.24.0",
        "eslint-config-apteryx": "^1.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "gen-esm-wrapper": "^1.1.3",
        "jest": "^29.0.3",
        "prettier": "^2.7.1",
        "prettier-config-apteryx": "^1.0.0",
        "ts-config-apteryx": "^1.0.0",
        "typescript": "<4.8.0",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "prettier": "prettier-config-apteryx",
    "eslintConfig": {
        "plugins": [
            "prettier"
        ],
        "extends": [
            "plugin:prettier/recommended",
            "apteryx",
            "apteryx/typescript"
        ],
        "parserOptions": {
            "ecmaVersion": 2021
        },
        "env": {
            "es2021": true,
            "node": true,
            "browser": true
        },
        "ignorePatterns": [
            "test/*",
            "examples/*",
            "scripts/*",
            "dist/*",
            "temp/*",
            "_src/*",
            "*.config.js"
        ]
    }
}
